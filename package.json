{
  "name": "data-browser",
  "version": "0.1.0",
  "license": "ISC",
  "scripts": {
    "test": "node ../../scripts/run-tests data-browser",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "format:write": "prettier **/*.{json,md} --write",
    "format:test": "prettier **/*.{json,md} --list-different",
    "release:major": "release-it major",
    "release:minor": "release-it minor",
    "release:patch": "release-it patch"
  },
  "description": "The data-browser module",
  "keywords": [
    "data-browser",
    "boilerplate-fm",
    "functional-module"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/izmjs/data-browser.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "lint-staged": "^9.5.0",
    "husky": "^3.1.0",
    "release-it": "^12.4.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "post-merge": "npm i",
      "pre-commit": "lint-staged && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "async": "^3.1.0",
    "express-handlebars": "^3.1.0",
    "junk": "^3.1.0",
    "moment": "^2.24.0",
    "mongo-uri": "^0.1.2",
    "mongodb": "^3.4.1",
    "mongodb-extended-json": "^1.10.2",
    "nconf": "^0.10.0",
    "nedb": "^1.8.0"
  }
}
